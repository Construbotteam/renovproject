<launch>
    <arg name="PATH_TO_VOCABULARY" value="$(find ORB_SLAM2)/../../../Vocabulary/ORBvoc.txt"/>
    <arg name="PATH_TO_SETTINGS_FILE" value="$(find paintingrobot_slam)/config/orbslam2_params.yaml "/>
    
    <node name="RGBD" pkg="ORB_SLAM2" type="RGBD" args="$(arg PATH_TO_VOCABULARY) $(arg PATH_TO_SETTINGS_FILE)">
   	    <remap from="/camera/depth_registered/image_raw" to="/kinect/depth/image_raw"/>
        <remap from="/camera/rgb/image_raw" to="/kinect/rgb/image_raw" />
    </node>
</launch>